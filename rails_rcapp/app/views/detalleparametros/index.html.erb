<p id="notice"><%= notice %></p>
<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
<h1>Detalle parametros</h1>

<table class="table table-hover table-responsive">
<thead class="thead-dark">
    <tr>
      <th>Nombre</th>
      <th>Parametros</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @detalleparametros.each do |detalleparametro| %>
      <tr>
        <td><%= detalleparametro.nombre %></td>
        <td><%= detalleparametro.parametros_id %></td>
        <td><%= link_to 'Show', detalleparametro %></td>
        <td><%= link_to 'Edit', edit_detalleparametro_path(detalleparametro) %></td>
        <td><%= link_to 'Destroy', detalleparametro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Detalleparametro', new_detalleparametro_path %>
