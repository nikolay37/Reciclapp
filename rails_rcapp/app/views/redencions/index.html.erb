<p id="notice"><%= notice %></p>

<h1>Redencions</h1>

<table>
  <thead>
    <tr>
      <th>Idredencion</th>
      <th>Idusuario</th>
      <th>Idpremio</th>
      <th>Fecharedencion</th>
      <th>Cantidadpremio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @redencions.each do |redencion| %>
      <tr>
        <td><%= redencion.idredencion %></td>
        <td><%= redencion.idusuario %></td>
        <td><%= redencion.idpremio %></td>
        <td><%= redencion.fecharedencion %></td>
        <td><%= redencion.Cantidadpremio %></td>
        <td><%= link_to 'Show', redencion %></td>
        <td><%= link_to 'Edit', edit_redencion_path(redencion) %></td>
        <td><%= link_to 'Destroy', redencion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Redencion', new_redencion_path %>
